{
    "name": "padhie/twitch-api-bundle",
    "version": "1.0.2",
    "type": "library",
    "keywords": [
        "twitch",
        "twitch.tv",
        "twitch-tv",
        "twitch-api",
        "api"
    ],
    "license": "MIT",
    "description": "A little collection to work with the Twitch-Api https://dev.twitch.tv/docs/v5/",
    "authors": [
        {
            "name": "padhie",
            "email": "admin@padhie.de"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "TwitchApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
